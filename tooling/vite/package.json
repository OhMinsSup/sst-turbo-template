{
  "name": "@template/vite",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build:lib": "tsup",
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@template/prettier-config",
  "devDependencies": {
    "@template/eslint-config": "workspace:*",
    "@template/prettier-config": "workspace:*",
    "@template/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "rimraf": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:"
  },
  "packageManager": "pnpm@9.0.5",
  "engines": {
    "bun": ">=1.0",
    "node": ">=20.0"
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./t3-env": {
      "import": {
        "types": "./dist/t3-env.d.ts",
        "default": "./dist/t3-env.js"
      },
      "require": {
        "types": "./dist/t3-env.d.js",
        "default": "./dist/t3-env.js"
      }
    },
    "./transpile": {
      "import": {
        "types": "./dist/transpile.d.ts",
        "default": "./dist/transpile.js"
      },
      "require": {
        "types": "./dist/transpile.d.js",
        "default": "./dist/transpile.js"
      }
    }
  },
  "dependencies": {
    "esbuild": "^0.23.0"
  }
}
