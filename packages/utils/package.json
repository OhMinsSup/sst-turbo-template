{
  "name": "@template/utils",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./date": {
      "import": {
        "types": "./dist/date.d.ts",
        "default": "./dist/date.js"
      },
      "require": {
        "types": "./dist/date.d.cts",
        "default": "./dist/date.cjs"
      }
    },
    "./assertion": {
      "import": {
        "types": "./dist/assertion.d.ts",
        "default": "./dist/assertion.js"
      },
      "require": {
        "types": "./dist/assertion.d.cts",
        "default": "./dist/assertion.cjs"
      }
    },
    "./jwt": {
      "import": {
        "types": "./dist/jwt.d.ts",
        "default": "./dist/jwt.js"
      },
      "require": {
        "types": "./dist/jwt.d.cts",
        "default": "./dist/jwt.cjs"
      }
    },
    "./cookie": {
      "import": {
        "types": "./dist/cookie.d.ts",
        "default": "./dist/cookie.js"
      },
      "require": {
        "types": "./dist/cookie.d.cts",
        "default": "./dist/cookie.cjs"
      }
    },
    "./request": {
      "import": {
        "types": "./dist/request.d.ts",
        "default": "./dist/request.js"
      },
      "require": {
        "types": "./dist/request.d.cts",
        "default": "./dist/request.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:pkg": "tsup",
    "clean": "rm -rf .turbo node_modules dist",
    "dev": "tsup --watch",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "prettier": "@template/prettier-config",
  "dependencies": {
    "cookie": "^0.6.0",
    "date-fns": "catalog:",
    "jose": "^5.6.3"
  },
  "devDependencies": {
    "@template/eslint-config": "workspace:*",
    "@template/prettier-config": "workspace:*",
    "@template/tsconfig": "workspace:*",
    "@types/cookie": "0.6.0",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
