{
  "name": "@template/api",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:pkg": "tsup",
    "clean": "rm -rf .turbo node_modules dist",
    "dev": "tsup --watch",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "prettier": "@template/prettier-config",
  "dependencies": {
    "@template/common": "workspace:*",
    "@template/utils": "workspace:*",
    "openapi-fetch": "^0.13.0"
  },
  "devDependencies": {
    "@template/api-types": "workspace:*",
    "@template/db": "workspace:*",
    "@template/eslint-config": "workspace:*",
    "@template/prettier-config": "workspace:*",
    "@template/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "openapi-typescript-helpers": "^0.0.15",
    "prettier": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
